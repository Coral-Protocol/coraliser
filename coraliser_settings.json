{
  "mcpServers": {
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": ""
      }
    },
    "firecrawl": {
        "transport": "sse",
        "url": "http://localhost:3000/sse",
        "timeout": 300,
        "sse_read_timeout": 300
    }
  }
}